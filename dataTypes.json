  "AbsoluteURI": {
    "ancestors": [
      "DataType",
      "Text"
    ],
    "comment": "A string which represents an absolute URI address (e.g. https://www.example.com/help?article=1001)",
    "id": "AbsoluteURI",
    "label": "AbsoluteURI",
    "properties": {
      "pattern":"^((https?|ftps?):\\/\\/)([\\da-z.-]+)\\.([a-z.]{2,6})([\\/\\w .-]*)*\\/?$"
    },
    "supertypes": [
      "Text"
    ],
    "type": "string",
    "sqlType":"Text"
  },
  "RelativeURI": {
    "ancestors": [
      "DataType",
      "Text"
    ],
    "comment": "A string which represents a relative URI address (e.g. /help?article=1001)",
    "id": "RelativeURI",
    "label": "RelativeURI",
    "properties": {
      "pattern":"^(\\/)([\\da-z.-]+)\\.([a-z.]{2,6})([\\/\\w .-]*)*\\/?$"
    },
    "supertypes": [
      "Text"
    ],
    "type": "string",
    "sqlType":"Text"
  },
  "IP": {
    "ancestors": [
