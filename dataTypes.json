  "Email": {
    "ancestors": [
      "DataType",
      "Text"
    ],
    "comment": "A string which represents an email address (e.g. user@example.com)",
    "id": "Email",
    "label": "Email",
    "properties": {
      "pattern":"^[A-Z0-9._%+-]+@[A-Z0-9.-]+\\.[A-Z]{2,}$"
    },
    "supertypes": [
      "Text"
    ],
    "type": "string",
    "sqlType":"Text"
